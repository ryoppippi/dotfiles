(defcfg
  process-unmapped-keys yes
  ;; CLAW44 shows as "/ Trackpad" with vendor_id 22854
  macos-dev-names-include ("/ Trackpad")
)

(include common.kbd)

;; CLAW44 keyboard layout (44 keys split keyboard)
;; Based on Layer 0 from QMK config
;; Only map keys we want to modify (Enter for app-specific, F5/F19 for Aqua Voice)
(defsrc
  f5   ret
)

(deflayer default
  @f5-f19 @enter-app
)
