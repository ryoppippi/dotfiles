{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ]
  },
  "extends": [
    "github>aquaproj/aqua-renovate-config#2.9.0"
  ],
  "labels": [
    "dependencies",
    "renovate"
  ],
  "platformAutomerge": true,
  "dependencyDashboard": false,
  "automerge": true,
  "branchConcurrentLimit": 0,
  "minimumReleaseAge": "3 days",
  "packageRules": [
    {
      "matchPackageNames": [
        "opencode",
        "codex",
        "bun",
        "deno"
      ],
      "stabilityDays": 0
    }
  ]
}
